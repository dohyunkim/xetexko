    \def\XeKo@maybe@gobble{\def\XeKo@maybe@gobble{\expandafter\expandafter\expandafter\@gobble}}%
    \expandafter\XeKo@flush@toks
  \else
    \ifnum\XeKo@currclass>\tw@
      \expandafter\expandafter\expandafter\XeKo@check@next@tok@latin
