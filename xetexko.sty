    \expandafter\가%
  \else
    \ifnum 0\ifnum\XeKo@charslot>"ABFF \ifnum\XeKo@charslot<"D7A4 1\fi\fi >\z@
      \expandafter\expandafter\expandafter\XeKo@make@josa@II
    \else
      \expandafter\expandafter\expandafter\가%
    \fi
